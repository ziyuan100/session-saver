{
    "name": "Session Saver",
    "version": "1.0.0",
    "description": "Saves the current Chrome session, to be loaded whenever it may be needed again",
    "manifest_version": 3,
    "author": "shsh",
    "action": {
        "default_popup": "main.html",
        "default_title": "Session Saver",
        "default_icon": "assets/save.png"
    },
    "permissions": [
        "tabs",
        "storage",
        "windows"
    ]
}